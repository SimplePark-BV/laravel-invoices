{
    "name": "simplepark-bv/laravel-invoices",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "keywords": [
        "simplepark",
        "laravel",
        "invoices"
    ],
    "authors": [
        {
            "name": "Lucas Castelein",
            "email": "lucas@simplepark.nl"
        },
        {
            "name": "Jasper Demmers",
            "email": "jasper@simplepark.nl"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "larastan/larastan": "^3.0",
        "laravel/sail": "^1.26",
        "orchestra/testbench": "^8.21.1|^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleParkBv\\LaravelInvoices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c ./phpunit.xml",
        "pint": "./vendor/bin/pint",
        "pint:test": "./vendor/bin/pint --test",
        "phpstan": "./vendor/bin/phpstan analyse"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleParkBv\\LaravelInvoices\\LaravelInvoicesServiceProvider"
            ]
        }
    }
}
